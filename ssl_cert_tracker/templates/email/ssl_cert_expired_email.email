{% block subject %}
Expired SSL Certificates Report as of {{report_date_time|date:"D, M d Y"}}
{% endblock %}

{% block html %}
<table color="#000000">
<tr align="left" valign="center">
{% for key, val in headers.items %}
<th><strong>{{val}}</strong></th>
{% endfor %}
</tr>
{% for item in data %}
<tr align="left" valign="center">
{% for value in item.values %}
<td>{{value}}</td>
{% endfor %}
</tr>
{% endfor %}
</table>
{% endblock %}